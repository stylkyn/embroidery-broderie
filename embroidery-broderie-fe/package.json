{
  "name": "embroidery-broderie-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "tsc && vite build",
    "preview": "vite preview --port 5173",
    "dn": "docker run -d --rm -p 5173:5173 --name dcsportselfstudy quay.io/livesport/dcsportselfstudy",
    "ds": "docker stop dcsportselfstudy",
    "de": "docker exec -it dcsportselfstudy bash",
    "dall": "yarn ds && yarn dn && yarn de && yarn build && yarn preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}